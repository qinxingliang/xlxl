<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="project.mapper.TCollectMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="project.entity.TCollect">
        <id column="c_id" property="cId" />
        <result column="c_cover" property="cCover" />
        <result column="c_title" property="cTitle" />
        <result column="d_price" property="dPrice" />
        <result column="c_spid" property="cSpid" />
        <result column="c_wx_open_id" property="cWxOpenId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        c_id, c_cover, c_title, d_price, c_spid, c_wx_open_id
    </sql>

</mapper>
