<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="project.mapper.TLbtMapper">

    <insert id="addOrUpdate" parameterType="project.entity.TLbt">
        replace into t_lbt (
             c_id
            ,c_img_url
            ,c_describe
            ,c_content)
        values (
         #{cId}
        ,#{cImgUrl}
        ,#{cDescribe}
        ,#{cContent})
    </insert>

</mapper>
