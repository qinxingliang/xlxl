<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="project.mapper.THssMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="project.entity.THss">
        <id column="c_id" property="cId" />
        <result column="c_name" property="cName" />
        <result column="c_phone" property="cPhone" />
        <result column="c_wx" property="cWx" />
        <result column="c_address" property="cAddress" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        c_id, c_name, c_phone, c_wx, c_address
    </sql>

</mapper>
